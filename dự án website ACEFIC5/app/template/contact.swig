{% extends 'layouts/base-subpage.swig' %}
{% block pageTitle %} Biệt thự trên cao Forest in the Sky {% endblock %}
{% block bodyClass %}index{% endblock %}
{% block content %}
<div id="main-news">
    <div class="section-path">
        <div class="bs-container">
            <div class="content-path">
                <div class="bs-col">
                    <div class="title-path">LIÊN HỆ</div>
                </div>
                <div class="bs-col">
                    <div class="link-path">
                        <a href="../../dist/index.html">Trang chủ</a>/ <a href="">Liên hệ</a>
                    </div>
                </div>
            </div>
        </div>

    </div>
    <div class="section-contact">
        <div class="bs-container">
            <div class="bs col">
                <div class="bs-row">
                    <div class="bs-col md-33">
                        <div class="icon-map"><i class="fas fa-map-marker-alt"></i></div>
                        <p class="content-text">Tầng 2, Lô 8+15 NV, Khu HH1
                            Ô16 Tây Hồ Tây,P.Xuân La, Q.Tây Hồ, TP. Hà Nội
                        </p>
                    </div>
                    <div class="bs-col md-33">
                        <div class="icon-phone"><i class="fas fa-phone"></i></div>
                        <p class="content-number">024.6666.7069</p>
                    </div>
                    <div class="bs-col md-33">
                        <div class="icon-email"><i class="far fa-envelope"></i></div>
                        <p class="content-text">vanphongace5.ace@gmail.com</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="section-map">
        <div class="bs-container">
            <div class="bs-row row-right">
                <div class="bs-col lg-50 md-50 xs-100">
                    <div class="form-infomation">
                        <div class="module">
                            <div class="module-header">
                                <div class="title-header">LIÊN HỆ</div>
                                <div class="content-header">Cung cấp thông tin để chúng tôi có thể hỗ trợ bạn</div>
                            </div>
                            <div class="module-content">
                                <div class="form-content">
                                    <form action="#" method="POST">
